#!/usr/bin/python
import base64
import hashlib
import sys

if len(sys.argv) != 2:
    sys.exit("Usage: mkpasswd seedfile")
else:
    filename = sys.argv[1]

f = file(filename)
buf = f.read()
f.close()
digest = hashlib.md5(buf).digest()
passwd = base64.encodestring(digest).replace("=", "")
print passwd,
